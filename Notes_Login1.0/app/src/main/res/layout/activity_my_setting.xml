<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_my_setting"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.joyyou.zhaojiaqizhao.notes_login.MySetting">

    <TextView
        android:text="我的信息"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/textView9" />

    <TextView
        android:text="用户名："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView9"
        android:layout_alignParentStart="true"
        android:layout_marginTop="43dp"
        android:id="@+id/textView10" />

    <TextView
        android:text="姓名："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView10"
        android:layout_alignParentStart="true"
        android:layout_marginTop="32dp"
        android:id="@+id/textView11" />

    <TextView
        android:text="生日："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView11"
        android:layout_alignEnd="@+id/textView11"
        android:layout_marginTop="38dp"
        android:id="@+id/textView12" />

    <TextView
        android:text="爱好："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView12"
        android:layout_alignParentStart="true"
        android:layout_marginTop="39dp"
        android:id="@+id/textView13" />

    <TextView
        android:text="地址："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView13"
        android:layout_alignParentStart="true"
        android:layout_marginTop="43dp"
        android:id="@+id/textView14" />

    <TextView
        android:text="xiaoming123"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView10"
        android:layout_alignStart="@+id/textView9"
        android:id="@+id/Text_Show_username" />

    <TextView
        android:text="小明"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView11"
        android:id="@+id/Text_Show_name"
        android:layout_alignEnd="@+id/Text_Show_username"
        android:layout_alignStart="@+id/Text_Show_username" />

    <TextView
        android:text="2000-1-1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView12"
        android:id="@+id/Text_Show_birthday"
        android:layout_alignEnd="@+id/Text_Show_name"
        android:layout_alignStart="@+id/Text_Show_name" />

    <TextView
        android:text="打篮球"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView13"
        android:id="@+id/Text_Show_interest"
        android:layout_alignEnd="@+id/Text_Show_birthday"
        android:layout_alignStart="@+id/Text_Show_birthday" />

    <TextView
        android:text="上海"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView14"
        android:id="@+id/Text_Show_location"
        android:layout_alignEnd="@+id/Text_Show_interest"
        android:layout_alignStart="@+id/Text_Show_interest" />

    <Button
        android:text="修改密码"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Button_Changepassword"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

</RelativeLayout>
